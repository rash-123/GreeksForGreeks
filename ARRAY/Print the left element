class Solution{
    public:
    int leftElement(int a[], int n) {
        // Your code goes here 
        sort(a, a+n);
        for(int i=0;i<n;i++){
            if(n%2==0){
                return a[(n/2)-1];
            }
            else{
                return a[n/2];
            }
        }
    }
};