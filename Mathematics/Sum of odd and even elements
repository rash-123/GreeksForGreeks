class Solution
{
	public:
		vector<int> find_sum(int n)
		{
		    // Code here
		    vector<int> ans;
		    int even = 0;
		    int odd = 0;
		    for(int i = 1; i <= n; i++){
		        if(i % 2 == 0){
		            even += i;
		        }
		        else{
		            odd += i;
		        }
		    }
		    ans.push_back(odd);
		    ans.push_back(even);
		    return ans;

		}
};