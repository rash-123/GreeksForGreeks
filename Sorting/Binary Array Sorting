class Solution{
  public:
    
    // A[]: input array
    // N: input array
    //Function to sort the binary array.
    void binSort(int A[], int N)
    {
       //Your code here
       
       /**************
        * No need to print the array
        * ************/
        int i = 0, j = 0;
         while(i <= N-1){
        if(A[i] == 0){
            swap(A[i], A[j]);
            i++;
            j++;
        }
        else if(A[i] == 1){
            i++;
        }
    }
    }
};