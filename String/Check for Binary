
// Return true if str is binary, else false
bool isBinary(string str)
{
   // Your code here
   long n = str.size();
   long count = 0;
   for(int i=0;i<n;i++){
       if(str[i] == '0' || str[i] == '1'){
           count++;
       }
   }
   if(count == n){
       return true;
   }
   return false;
}