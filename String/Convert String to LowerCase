// User function Template for C++

class Solution {
  public:
    string toLower(string S) {
        // code here
        int n = S.size();
        for(int i=0;i<n;i++){
            if(S[i] >= 'A' && S[i] <= 'Z'){
                S[i]= S[i] + ('a' - 'A');
            }
        }
        return S;
        
    }
};