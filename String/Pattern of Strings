vector<string> pattern(string S)
	{
	    // Your code goes here
	    vector<string> ans;
	    int n = S.size();
	    string temp = "";
	    for(int i=0;i<n;i++){
	        temp = "";
	        for(int j=0;j<=n-1-i;j++){
	            temp = temp + S[j];
	        }
	        ans.push_back(temp);
	    }
	    return ans;
	}